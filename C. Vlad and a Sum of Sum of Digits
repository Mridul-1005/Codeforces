#include <bits/stdc++.h>
using namespace std;
#define int long long 


signed main()
{

  int t;
  cin >> t;
  int N=2*1e5;
  int a[N]={0};
  int sum=0;
for(int i=1;i<=N;i++){
int c=0;
    int j=i;
while(j>0){
    int l=j%10;
    j=j/10;
    c+=l;
}
sum+=c;
a[i]=sum;
}
  while (t--)
  {
int n;
cin>>n;


cout<<a[n]<<endl;
}
}
